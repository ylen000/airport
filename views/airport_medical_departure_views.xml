<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="airport_medical_departure_action" model="ir.actions.act_window">
        <field name="name">啟程地</field>
        <field name="res_model">airport.medical.departure</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="airport_medical_departure_view_tree" model="ir.ui.view">
        <field name="name">airport.medical.departure.tree</field>
        <field name="model">airport.medical.departure</field>
        <field name="arch" type="xml">
            <tree string="Channel" editable="bottom">
                <field name="name" string="啟程地名稱"/>
            </tree>
        </field>
    </record>

    <record id="airport_medical_departure_view_form" model="ir.ui.view">
        <field name="name">airport.medical.departure.form</field>
        <field name="model">airport.medical.departure</field>
        <field name="arch" type="xml">
            <form string="Test">
                <sheet>
                    <group>
                        <field name="name" string="啟程地名稱"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>