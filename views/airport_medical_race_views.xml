<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="airport_medical_race_action" model="ir.actions.act_window">
        <field name="name">種族</field>
        <field name="res_model">airport.medical.race</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="airport_medical_race_view_tree" model="ir.ui.view">
        <field name="name">airport.medical.race.tree</field>
        <field name="model">airport.medical.race</field>
        <field name="arch" type="xml">
            <tree string="Channel" editable="bottom">
                <field name="name" string="種族名稱"/>
            </tree>
        </field>
    </record>

    <record id="airport_medical_race_view_form" model="ir.ui.view">
        <field name="name">airport.medical.race.form</field>
        <field name="model">airport.medical.race</field>
        <field name="arch" type="xml">
            <form string="Test">
                <sheet>
                    <group>
                        <field name="name" string="種族名稱"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>